#include<iostream>
int main(){
	int sum = 0, val = 1;
	while (val <= 10000){
		sum += val;
		++val;
	}
	std::cout << "sum of 1 to 10 inclusive is"
		<< sum << std::endl;
	return 0;
}
#include<iostream>
int main(){
	int sum = 0, val = 10;
	while (val >=0){
		sum += val;
		--val;
	}
	std::cout << "sum of 10 to 0 inclusive is"
		<< sum << std::endl;
	return 0;
}
#include<iostream>
int main(){
	int v1, v2, v3;
	std::cin >> v1 >> v2;
	while (v1 <= v2){
	std::cout << v1 << std::endl;
	v1++;
	}while (v2 <=v1){
		std::cout << v2 << std::endl;
		v2++;
	}
	return 0;

}
