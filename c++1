int main(){
	for (int i = 1; i < 100; i++){
		if(i % 7 == 0 || i % 10 == 7 || i / 10 == 7){
			cout << i   <<  "敲桌子" << endl;
		}
		else
		cout << i << endl;
	}
	return 0;
}
Status GetElem(LinkList L, int i, ElemType &e){
	p->next; j = 1;
	while (p&&j < i){
		p = p->next;
		++j;
	}
	if (!p || j>1)return ERROR;
	e = p->data;
	return ok;
}
